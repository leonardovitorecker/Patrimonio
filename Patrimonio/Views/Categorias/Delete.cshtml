@model Patrimonio.Models.DbCategoria

@{
    Layout = "_layoutCategoria";
    ViewData["Categorias"] = "Delete";
}

<h1>Deletar</h1>

<h3>Voce deseja deletar realmente?</h3>
<div>
  
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.descricaocategoria)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.descricaocategoria)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="id" />
        <input type="submit" value="Deletar" class="btn btn-danger" /> 
        <a asp-action="Index"> <input type="submit" value="Voltar para lista" class="btn btn-primary " /></a>
    </form>
</div>
