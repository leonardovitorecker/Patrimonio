@model Patrimonio.Models.DtoPatrimonio

@{
     Layout = "_LayoutPatrimonio";
    ViewData["Patrimonios"] = "Details";
}
<h1>Detalhes</h1>

<div>
    <hr />
    <dl class="row">
         <dt class = "col-sm-2">
           Nome patrimonio
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.nomepatrimonio)
        </dd>
        <dt class = "col-sm-2">
           Número etiqueta
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.numeetiqueta)
        </dd>
        <dt class = "col-sm-2">
           Descrição do patrimonio
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.descricaopatrimonio)
        </dd>
        <dt class = "col-sm-2">
           Valor patrimonio
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.valorpatrimonio)
        </dd>
        <dt class = "col-sm-2">
            Categoria
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.nomecategoria)
        </dd>
        <dt class = "col-sm-2">
           Local
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.nomelocal)
        </dd>
         <dt class = "col-sm-2">
            Fornecedor
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.nomefornecedor)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.marcamodelo)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.marcamodelo)
        </dd>
       <dt class = "col-sm-2">
           Situação
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.situacao)
        </dd>
        <dt class = "col-sm-2">
           Número de serie
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.numserie)
        </dd>
         <dt class = "col-sm-2">
            Número
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.numf)
        </dd>
        <dt class = "col-sm-2">
          Data aquisição
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.dataaquisicao)
        </dd>
        <dt class = "col-sm-2">
           Data da baixa
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.databaixa)
        </dd>
        <dt class = "col-sm-2">
          Data garantia
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.datagarantia)
        </dd>
    </dl>
</div>
<div>
     <a asp-action="Edit" asp-route-id="@Model?.id"> <input type="submit" value="Editar" class="btn btn-primary " ></a>
    <a asp-action="Index"> 
        <input type="submit" value="Voltar para lista" class="btn btn-primary " ></a>
</div>
